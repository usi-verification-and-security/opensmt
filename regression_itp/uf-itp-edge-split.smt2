(set-option :produce-interpolants true)
(set-option :certify-interpolants true)
(set-logic QF_UF)
(declare-sort U 0)
(declare-fun p () Bool)
(declare-fun q () Bool)
(declare-fun r () Bool)
(declare-fun s () U)
(declare-fun u () U)
(declare-fun v () U)
(declare-fun x () U)
(declare-fun C (Bool U) U)
(assert (! (and (= x (C p u)) p) :named A1))
(assert (! (and q r) :named B1))
(assert (! (and (= u s)) :named A2))
(assert (! (and (= s (C q u)) (= v (C r u)) (not (= u v))) :named B2))
(check-sat)
(get-interpolants (and A1 A2) (and B1 B2))
