(set-option :produce-unsat-cores true)
(set-logic QF_LIA)
(declare-fun o () Int)
(declare-fun i () Bool)
(assert (or (not i) (< o 0)))
(push 1)
(assert (! i :named c0))
(check-sat)
(pop 1)
(declare-fun __ () Bool)
(assert (or (>= o 0) (not __)))
(assert (! __ :named c15))
(assert (! i :named c17))
(check-sat)
(get-unsat-core)
