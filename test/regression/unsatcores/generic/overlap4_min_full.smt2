(set-option :produce-unsat-cores true)
(set-option :minimal-unsat-cores true)
(set-option :print-cores-full true)
(set-logic QF_UF)
(declare-fun b1 () Bool)
(declare-fun b2 () Bool)
(assert (! b1 :named a1))
(assert (! (and b1 b2) :named a2))
(assert (or (not b1) (not b2)))
(check-sat)
(get-unsat-core)
(exit)
